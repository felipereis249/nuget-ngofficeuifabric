{
  "name": "nuget-ngofficeuifabric",
  "description": "NuGet package distribution for ngOfficeUiFabric",
  "version": "0.5.0",
  "scripts": {
    "build": "npm run update:tsd && npm run compile:ts",
    "update:tsd": "tsd reinstall -ros",
    "compile:ts": "tsc -p ./",
    "clean": "npm run compile:ts && gulp clean"
  },
  "author": "https://github.com/ngOfficeUIFabric/ng-officeuifabric/graphs/contributors",
  "maintainers": [
    {
      "name": "andrewconnell",
      "email": "me@andrewconnell"
    }
  ],
  "license": "MIT",
  "keywords": [
    "angular",
    "office",
    "office-ui-fabric",
    "client-side",
    "office-365",
    "office365",
    "sharepoint"
  ],
  "homepage": "https://ngOfficeUiFabric.com",
  "bugs": {
    "url": "https://github.com/ngOfficeUIFabric/ng-officeuifabric/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngOfficeUIFabric/ng-officeuifabric.git"
  },
  "devDependencies": {
    "archiver": "^0.21.0",
    "chalk": "^1.1.1",
    "gulp": "^3.9.1",
    "gulp-debug": "^2.1.2",
    "gulp-help": "^1.6.1",
    "gulp-if": "^2.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-print": "^2.0.1",
    "gulp-rimraf": "^0.2.0",
    "gulp-tslint": "^4.3.2",
    "gulp-typescript": "^2.11.0",
    "gulp-util": "^3.0.7",
    "request": "^2.69.0",
    "run-sequence": "^1.1.5",
    "tslint": "^3.3.0",
    "yargs": "^4.1.0"
  }
}
